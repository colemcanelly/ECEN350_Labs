S_0x5555841d50 .scope module, "Decode24Test_v" "Decode24Test_v" 2 26;
 .timescale -9 -12;
v0x5555895db0_0 .var "in", 1 0;
v0x5555895ea0_0 .net "out", 3 0, v0x5555895c70_0;  1 drivers
v0x5555895f70_0 .var "passed", 7 0;
S_0x5555841ee0 .scope task, "allPassed" "allPassed" 2 41, 2 41 0, S_0x5555841d50;
 .timescale -9 -12;
v0x55558420c0_0 .var "numTests", 7 0;
v0x55558952e0_0 .var "passed", 7 0;
TD_Decode24Test_v.allPassed ;
    %load/vec4 v0x55558952e0_0;
    %load/vec4 v0x55558420c0_0;
    %cmp/e;
    %jmp/0xz  T_0.0, 4;
    %vpi_call 2 45 "$display", "All tests passed" {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 46 "$display", "Some tests failed" {0 0 0};
T_0.1 ;
    %end;
S_0x55558953c0 .scope task, "passTest" "passTest" 2 32, 2 32 0, S_0x5555841d50;
 .timescale -9 -12;
v0x55558955c0_0 .var "actualOut", 3 0;
v0x55558956a0_0 .var "expectedOut", 3 0;
v0x5555895780_0 .var "passed", 7 0;
v0x5555895840_0 .var "testType", 120 0;
TD_Decode24Test_v.passTest ;
    %load/vec4 v0x55558955c0_0;
    %load/vec4 v0x55558956a0_0;
    %cmp/e;
    %jmp/0xz  T_1.2, 4;
    %vpi_call 2 37 "$display", "%s passed", v0x5555895840_0 {0 0 0};
    %load/vec4 v0x5555895780_0;
    %addi 1, 0, 8;
    %store/vec4 v0x5555895780_0, 0, 8;
    %jmp T_1.3;
T_1.2 ;
    %vpi_call 2 38 "$display", "%s failed: %d should be %d", v0x5555895840_0, v0x55558955c0_0, v0x55558956a0_0 {0 0 0};
T_1.3 ;
    %end;
S_0x5555895920 .scope module, "uut" "Decode24" 2 57, 3 1 0, S_0x5555841d50;
 .timescale -9 -12;
    .port_info 0 /OUTPUT 4 "out";
    .port_info 1 /INPUT 2 "in";
v0x5555895b70_0 .net "in", 1 0, v0x5555895db0_0;  1 drivers
v0x5555895c70_0 .var "out", 3 0;
E_0x5555880420 .event edge, v0x5555895b70_0;
    .scope S_0x5555895920;
T_2 ;
    %wait E_0x5555880420;
    %load/vec4 v0x5555895b70_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_2.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_2.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_2.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_2.3, 6;
    %jmp T_2.4;
T_2.0 ;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x5555895c70_0, 0, 4;
    %jmp T_2.4;
T_2.1 ;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x5555895c70_0, 0, 4;
    %jmp T_2.4;
T_2.2 ;

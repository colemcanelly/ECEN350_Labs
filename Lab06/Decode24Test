    %jmp T_2.4;
T_2.3 ;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x5555895c70_0, 0, 4;
    %jmp T_2.4;
T_2.4 ;
    %pop/vec4 1;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x5555841d50;
T_3 ;
    %vpi_call 2 29 "$dumpfile", "Decode24Test.vcd" {0 0 0};
    %vpi_call 2 30 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5555841d50 {0 0 0};
    %end;
